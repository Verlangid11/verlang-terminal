FROM debian:bullseye-slimLABEL author="Mr." maintainer="verlangid@gmail.com"ENV DEBIAN_FRONTEND=noninteractive \    USER=container \    HOME=/home/container \    NODE_INSTALL_DIR=/usr/local/node \    BUN_INSTALL=/usr/local/bun \    PLAYWRIGHT_BROWSERS_PATH=/usr/local/share/playwright \    GO_VERSION=1.24.0 \    PYTHON_VERSION=3.13.1 \    RUSTUP_HOME=/usr/local/rustup \    CARGO_HOME=/usr/local/cargo \    JAVA_HOME=/usr/local/java \    DOTNET_ROOT=/usr/local/dotnet \    PHP_VERSION=8.3ENV PATH="\( {NODE_INSTALL_DIR}/bin: \){BUN_INSTALL}/bin:/usr/local/go/bin:\( {CARGO_HOME}/bin: \){DOTNET_ROOT}:\( {DOTNET_ROOT}/tools:/usr/local/bin: \){JAVA_HOME}/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"RUN apt-get update && apt-get install -y --no-install-recommends \    curl wget git zip unzip tar gzip bzip2 p7zip-full zstd jq nano vim ca-certificates gnupg \    net-tools iputils-ping dnsutils procps build-essential make gcc g++ libssl-dev zlib1g-dev \    libbz2-dev libreadline-dev libsqlite3-dev libncursesw5-dev xz-utils tk-dev libffi-dev liblzma-dev \    ffmpeg webp fonts-liberation fonts-noto-color-emoji libfontconfig1 libfreetype6 libasound2 libgbm1 \    libgtk-3-0 libnss3 libnspr4 libatk1.0-0 libatk-bridge2.0-0 libcups2 libdrm2 libdbus-1-3 libexpat1 \    libx11-xcb1 libxcb-dri3-0 libxss1 libxtst6 \    && mkdir -p /usr/share/keyrings \    && curl -fsSL https://pkg.cloudflare.com/cloudflare-public-v2.gpg | gpg --dearmor -o /usr/share/keyrings/cloudflare.gpg \    && echo 'deb [signed-by=/usr/share/keyrings/cloudflare.gpg] https://pkg.cloudflare.com/cloudflared bullseye main' > /etc/apt/sources.list.d/cloudflared.list \    && apt-get update && apt-get install -y cloudflared \    && rm -rf /var/lib/apt/lists/*RUN curl -fsSL https://go.dev/dl/go${GO_VERSION}.linux-amd64.tar.gz | tar -C /usr/local -xzf - \    && rm -f go*.tar.gzRUN curl -fsSL https://www.python.org/ftp/python/\( {PYTHON_VERSION}/Python- \){PYTHON_VERSION}.tgz | tar xz -C /tmp \    && cd /tmp/Python-${PYTHON_VERSION} \    && ./configure --enable-optimizations --prefix=/usr/local \    && make -j$(nproc) altinstall \    && ln -sf /usr/local/bin/python3.13 /usr/local/bin/python3 \    && ln -sf /usr/local/bin/pip3.13 /usr/local/bin/pip3 \    && cd / && rm -rf /tmp/Python-${PYTHON_VERSION}*RUN curl -fsSL https://github.com/oven-sh/bun/releases/latest/download/bun-linux-x64.zip -o /tmp/bun.zip \    && unzip /tmp/bun.zip -d /tmp/bun \    && mkdir -p ${BUN_INSTALL}/bin \    && mv /tmp/bun/bun ${BUN_INSTALL}/bin/bun \    && chmod +x ${BUN_INSTALL}/bin/bun \    && rm -rf /tmp/bun*RUN curl -fsSL https://deno.land/install.sh | sh \    && mv /root/.deno/bin/deno /usr/local/bin/deno \    && chmod +x /usr/local/bin/denoRUN curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y --no-modify-path \    && chmod -R 777 ${RUSTUP_HOME} ${CARGO_HOME}RUN mkdir -p ${JAVA_HOME} \    && curl -fsSL "https://github.com/adoptium/temurin21-binaries/releases/download/jdk-21.0.9%2B10/OpenJDK21U-jdk_x64_linux_hotspot_21.0.9_10.tar.gz" -o /tmp/java.tar.gz \    && tar -xzf /tmp/java.tar.gz --strip-components=1 -C ${JAVA_HOME} \    && rm /tmp/java.tar.gz \    && ln -s ${JAVA_HOME}/bin/java /usr/local/bin/java \    && ln -s ${JAVA_HOME}/bin/javac /usr/local/bin/javacRUN curl -sSLo /tmp/setup-php.sh https://packages.sury.org/php/setup.sh && bash /tmp/setup-php.sh \    && apt-get install -y php\( {PHP_VERSION} php \){PHP_VERSION}-cli php\( {PHP_VERSION}-mbstring php \){PHP_VERSION}-xml php\( {PHP_VERSION}-curl php \){PHP_VERSION}-zip php\( {PHP_VERSION}-gd php \){PHP_VERSION}-intl \    && curl -sS https://getcomposer.org/installer | php -- --install-dir=/usr/local/bin --filename=composer \    && rm /tmp/setup-php.shRUN curl -sSL https://dot.net/v1/dotnet-install.sh | bash /dev/stdin --channel 8.0 --install-dir ${DOTNET_ROOT} \    && ln -s ${DOTNET_ROOT}/dotnet /usr/local/bin/dotnetRUN curl -L https://luarocks.org/releases/luarocks-3.11.1.tar.gz | tar xz \    && cd luarocks-3.11.1 \    && ./configure --prefix=/usr/local \    && make && make install \    && cd .. && rm -rf luarocks* \    && luarocks install luasocketRUN mkdir -p ${PLAYWRIGHT_BROWSERS_PATH} \    && curl -fsSL https://deb.nodesource.com/setup_20.x | bash - \    && apt-get install -y nodejs \    && npm install -g playwright \    && npx playwright install --with-deps \    && npm cache clean --force \    && apt-get purge -y nodejs && apt-get autoremove -y --purgeRUN useradd -m -d ${HOME} -s /bin/bash ${USER} \    && mkdir -p ${NODE_INSTALL_DIR} ${PLAYWRIGHT_BROWSERS_PATH} ${RUSTUP_HOME} ${CARGO_HOME} ${JAVA_HOME} ${DOTNET_ROOT} \    && chown -R \( {USER}: \){USER} ${NODE_INSTALL_DIR} ${PLAYWRIGHT_BROWSERS_PATH} ${HOME} ${RUSTUP_HOME} ${CARGO_HOME} ${JAVA_HOME} ${DOTNET_ROOT}USER ${USER}WORKDIR ${HOME}COPY entrypoint.sh /entrypoint.shRUN chmod +x /entrypoint.shCMD ["/entrypoint.sh"]